    <display-name>TestConnexion</display-name>
    <servlet-name>TestConnexion</servlet-name>
    <servlet-class>fr.eni_ecole.jee.servlet.TestConnexion</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestConnexion</servlet-name>
    <url-pattern>/TestConnexion</url-pattern>
   </servlet-mapping>
   <servlet>
    <display-name>AuthentificationCandidat</display-name>
